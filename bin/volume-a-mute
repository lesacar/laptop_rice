#!/bin/bash
state=$(amixer | grep Master -a5 | grep "Mono: Playback" | awk '{ print $6}' | sed 's/[][]//g')
echo $state
if [[ $state == 'off' ]]; then
	amixer sset Master unmute && state='on' && dunstctl close-all && notify-send $state

elif [[ $state == 'on' ]]; then
	amixer sset Master mute && state='off' && dunstctl close-all && notify-send $state

fi
